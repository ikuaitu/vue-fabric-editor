{
  "name": "vue3-fabric-editor",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "npm run format &&  run-p  build-only",
    "preview": "npm run format && vite preview",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "pinia": "^2.0.32",
    "uuid": "^9.0.0",
    "view-ui-plus": "^1.3.1",
    "vue": "^3.2.47",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.0",
    "@types/uuid": "^9.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "typescript": "~4.7.4",
    "vite": "^4.1.3",
    "vue-tsc": "^1.1.5"
  }
}
