{
  "name": "view-ui-project-ts",
  "private": true,
  "version": "1.0.0",
  "description": "a template project for vue3, ViewUIPlus, TypeScript and Vite.",
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite serve",
    "dev:staging": "vite serve --mode=staging",
    "dev:prod": "vite serve --mode=production",
    "build": "vite build",
    "build:staging": "vite build --mode=staging",
    "preview": "npm run build && vite preview",
    "preview:staging": "npm run build:staging && vite preview --mode=staging"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "events": "^3.3.0",
    "fabric": "^5.2.1",
    "fontfaceobserver": "^2.1.0",
    "hotkeys-js": "^3.8.8",
    "lodash-es": "^4.17.21",
    "lodash.get": "^4.4.2",
    "uuid": "^8.3.2",
    "view-ui-plus": "^1.3.1",
    "vue": "^3.2.25",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.16",
    "vue3-lazyload": "^0.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/fabric": "^4.5.15",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "autoprefixer": "^10.4.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "husky": "^8.0.0",
    "less": "^4.0.0",
    "less-loader": "^10.2.0",
    "prettier": "2.8.4",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vue-tsc": "^0.34.7",
    "lint-staged": "^13.1.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}