    "vue-router": "^4.0.16",
    "vue3-lazyload": "^0.3.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "autoprefixer": "^10.4.5",
